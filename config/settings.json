{
    "window.restoreWindows": "all",
    "editor.suggestSelection": "first",
    "workbench.iconTheme": "material-icon-theme",
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "security.workspace.trust.startupPrompt": "never",
    "settingsSync.ignoredExtensions": [
        "ms-toolsai.jupyter",
        "tht13.python",
        "dongli.python-preview",
        "formulahendry.code-runner"
    ],
    "eslint.run": "onSave",
    "eslint.alwaysShowStatus": true,
    "eslint.codeActionsOnSave.rules": null,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.sortImports": true
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.formatOnSave": true,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 160,
    "prettier.singleQuote": true,
    "prettier.enable": true,
    "prettier.tabWidth": 4,
    "git.enableSmartCommit": true,
    "debug.javascript.autoAttachFilter": "always",
    "git.confirmSync": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "window.zoomLevel": 1,
    "database-client.highlightSQLBlock": true,
    "database-client.showQuery": true,
    "database-client.showView": false,
    "database-client.showFilter": true,
    "editor.fontSize": 16,
    "editor.mouseWheelZoom": true,
    "editor.linkedEditing": true,
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "workbench.colorCustomizations": {
        "tab.activeBorder": "#ff0000",
        "tab.unfocusedActiveBorder": "#000000"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "[typescriptreact]": {},
    "files.eol": "\n",
    "editor.stickyScroll.enabled": true,
    "files.trimTrailingWhitespace": true,
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "workbench.colorTheme": "Community Material Theme High Contrast",
    "workbench.preferredDarkColorTheme": "Community Material Theme High Contrast",
    "workbench.preferredHighContrastColorTheme": "Community Material Theme High Contrast",
    "git.autofetch": true,
    "editor.guides.bracketPairs": "active",
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#ffee00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "highlight",
            "bold": false,
            "italic": false
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        }
    ]
}
